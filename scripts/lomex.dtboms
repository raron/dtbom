name=LomEx
searchIdScript="ret = 'http://lomex.hu/hu/webshop/#/page,0/search,' + dtBomId + '/stype,5/sidx,1'"
searchScript="function insertSelectLinks()\n{\n    var lista = document.getElementsByClassName('item_content ');\n    if (document.getElementsByClassName('selectid').length == 0)\n    {\n        var i;\n        \n        var disp = document.body.style.display;\n        \n        document.body.style.display = 'none';\n        for(i = 0; i < lista.length; i++)\n        {\n            var leiras = lista[i].getElementsByClassName('item_desc')[0];\n            var id = leiras.getElementsByTagName('b')[0].innerHTML;\n            \n            var link = document.createElement('a');\n            link.className = 'selectid'\n            link.style.color = 'red';\n            link.style.fontWeight = 'bold';\n            link.appendChild(document.createTextNode(' SELECT'));\n            link.href = 'selectid:' + id;\n            leiras.appendChild(link);\n        }\n        document.body.style.display = disp || '';\n    }\n}\n\nsetInterval(insertSelectLinks, 300);"
searchUrlScript="if (dtBomValue != '')\n{\n   ret = 'http://lomex.hu/hu/webshop/#/page,0/search,' + dtBomValue + '/stype,5/sidx,1'\n}\nelse\n{\n   ret = 'http://lomex.hu/hu/webshop/#/page,0/search,' + dtBomType + '/stype,5/sidx,1'\n};\n\n"
version=1
