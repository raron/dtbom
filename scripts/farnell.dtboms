searchScript="\nfunction insertSelectLinks()\n{\n    if (document.getElementsByClassName('selectid').length == 0)\n    {\n        var disp = document.body.style.display;\n        document.body.style.display = 'none';\n\n        if ((document.getElementById('searchResults') != null) && (document.getElementById('searchResults').tBodies.length >= 2))\n        {\n            var rows = document.getElementById('searchResults').tBodies[1].rows;\n\n\n            var i;\n            for(i = 0; i < rows.length; i+=2)\n            {\n                var id = rows[i].cells[1].getElementsByTagName('a')[0].innerHTML\n\n                var link = document.createElement('a');\n                link.className = \"selectid\"\n                link.style.color = 'red';\n                link.style.fontWeight = 'bold';\n                link.appendChild(document.createTextNode(' SELECT'));\n                link.href = \"selectid:\" + id;\n                rows[i].cells[0].appendChild(link);\n            }\n        }\n\n\n        if (document.getElementById('pdContainer') != null)\n        {\n\n            var idnode = document.getElementById('pdContainer').getElementsByTagName('dd')[1];\n            var id = idnode.innerHTML;\n            var link = document.createElement('a');\n            link.className = \"selectid\"\n            link.style.color = 'red';\n            link.style.fontWeight = 'bold';\n            link.appendChild(document.createTextNode(' SELECT'));\n            link.href = \"selectid:\" + id;\n            idnode.parentNode.appendChild(link);\n\n        }\n\n        document.body.style.display = disp || '';\n    }\n}\n\nsetInterval(insertSelectLinks, 300);\n\n"
searchUrlScript="if (dtBomValue != '')\n{\n   ret = 'http://hu.farnell.com/jsp/search/browse.jsp;?N=0&Ntk=gensearch&Ntt=' + dtBomValue + '&Ntx=mode+matchallpartial&exposeLevel2Refinement=true&suggestions=false&ref=globalsearch';\n}\nelse\n{\n   ret = 'http://hu.farnell.com/jsp/search/browse.jsp;?N=0&Ntk=gensearch&Ntt=' + dtBomType + '&Ntx=mode+matchallpartial&exposeLevel2Refinement=true&suggestions=false&ref=globalsearch';\n};\n"
