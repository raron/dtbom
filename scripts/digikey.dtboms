name=DigiKey
searchIdScript="ret = 'http://www.digikey.com/product-search/en?keywords=' + dtBomId + '&WT.z_header=link'"
searchScript="var disp = document.body.style.display;\n\ndocument.body.style.display = 'none';\n\nif (document.getElementById('productTable') != null)\n{\n    var sorok = document.getElementById('productTable').tBodies[0].rows;\n    var i;\n    for(i = 0; i < sorok.length; i++)\n    {\n        var id = sorok[i].cells[3].getElementsByTagName('a')[0].innerHTML\n\n        var link = document.createElement('a');\n        link.className = 'selectid'\n        link.style.color = 'red';\n        link.style.fontWeight = 'bold';\n        link.appendChild(document.createTextNode(' SELECT'));\n        link.href = 'selectid:' + id;\n        sorok[i].cells[0].appendChild(link);\n    }\n}\nif (document.getElementById('reportpartnumber') != null)\n{\n    var id2node = document.getElementById('reportpartnumber');\n    var id2 = id2node.innerHTML.replace(/<.*>/g, '');\n    var link2 = document.createElement('a');\n    link2.className = 'selectid'\n    link2.style.color = 'red';\n    link2.style.fontWeight = 'bold';\n    link2.appendChild(document.createTextNode(' SELECT'));\n    link2.href = 'selectid:' + id2;\n    id2node.appendChild(link2);\n}\ndocument.body.style.display = disp || '';\n"
searchUrlScript="if (dtBomValue != '')\n{\n   ret = 'http://www.digikey.com/product-search/en?keywords=' + dtBomValue + '&WT.z_header=link'\n}\nelse\n{\n   ret = 'http://www.digikey.com/product-search/en?keywords=' + dtBomType + '&WT.z_header=link'\n};\n\n"
version=1
