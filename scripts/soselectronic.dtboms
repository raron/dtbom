searchIdScript="ret = 'http://www.soselectronic.com/?str=371&artnum=' + dtBomId"
searchScript="\nvar disp = document.body.style.display;\n\ndocument.body.style.display = 'none';\n\nif (document.getElementById('prod_table') != null)\n{\n    var sorok = document.getElementById('prod_table').tBodies[0].rows;\n    var i;\n    for(i = 0; i < sorok.length; i++)\n    {\n        var id = sorok[i].cells[1].innerHTML.split('\\n')[7].replace(/\\,.*/, '').replace(/.*: /, '');\n\n        var link = document.createElement('a');\n        link.className = \"selectid\"\n        link.style.color = 'red';\n        link.style.fontWeight = 'bold';\n        link.appendChild(document.createTextNode(' SELECT'));\n        link.href = \"selectid:\" + id;\n        sorok[i].cells[1].appendChild(link);\n    }\n}\nif (document.getElementById('prod_description_table') != null)\n{\n    var id2node = document.getElementById('prod_description_table').tBodies[0].rows[5].cells[1];\n    var id2 = id2node.innerHTML;\n    var link2 = document.createElement('a');\n    link2.className = \"selectid\"\n    link2.style.color = 'red';\n    link2.style.fontWeight = 'bold';\n    link2.appendChild(document.createTextNode(' SELECT'));\n    link2.href = \"selectid:\" + id2;\n    id2node.appendChild(link2);\n}\ndocument.body.style.display = disp || '';\n"
